{
  "name": "js-tracker",
  "version": "2.1.3",
  "description": "A chrome extension tracks front-end JavaScript using DOM or jQuery APIs to manipulate html dom elements at runtime.",
  "main": "index.js",
  "scripts": {
    "test": "karma start ./karma/tracker.js --single-run & karma start ./karma/trackerModules.js --single-run & karma start ./karma/extension.js --single-run",
    "test-tracker": "karma start ./karma/tracker.js",
    "test-tracker-modules": "karma start ./karma/trackerModules.js",
    "test-extension": "karma start ./karma/extension.js",
    "build": "node webpack/compiler.js",
    "watch": "webpack --watch",
    "dev": "webpack-dev-server --port 3000",
    "deploy": "rm ~/Desktop/JS-Tracker.zip & zip -r ~/Desktop/JS-Tracker.zip manifest.json assets/ background.js devtools.* contentscript-dist.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pilagod/js-tracker.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "web",
    "html",
    "css",
    "javascript",
    "dom",
    "jquery",
    "manipulation",
    "debug"
  ],
  "author": "pilagod <pilagooood@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/pilagod/js-tracker",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^3.5.1",
    "@types/chrome": "0.0.47",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.14",
    "@types/react": "^15.0.39",
    "@types/react-dom": "^15.5.1",
    "@types/sinon": "^2.3.3",
    "@types/stacktrace-js": "0.0.32",
    "awesome-typescript-loader": "^3.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.24.1",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^13.3.0",
    "chai": "^3.5.0",
    "diff-dom": "^2.1.0",
    "es6-promise": "^3.3.1",
    "file-loader": "^0.11.2",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^0.4.5",
    "karma": "^1.6.0",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-clear-screen-reporter": "^1.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-typescript": "^3.0.4",
    "karma-typescript-es6-transform": "^1.0.0",
    "mocha": "^3.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "sinon": "^2.4.1",
    "stackframe": "^1.0.3",
    "stacktrace-js": "^2.0.0",
    "transform-loader": "^0.2.4",
    "typescript": "^2.4.1",
    "watchify": "^3.9.0",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  },
  "browserify": {
    "options": {
      "browserifyOptions": {
        "debug": true
      }
    },
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-3"
          ]
        }
      ],
      "brfs"
    ]
  }
}